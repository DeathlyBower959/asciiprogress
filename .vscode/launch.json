{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "runtimeArgs": [
        "--exec",
        "\"node --nolazy -r ts-node/register/transpile-only\""
      ],
      "args": ["src/test/index.ts"],
      // "preLaunchTask": "npm:dev",
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}
